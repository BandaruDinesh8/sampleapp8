{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "types": [],
    "resolveJsonModule": true,
    "noImplicitOverride": false,
    "target": "es2020",
    "module": "es2020",
    "importHelpers": true,
    "lib": ["es2020", "dom"]
  },
  "exclude": ["src/test-setup.ts", "**/*.spec.ts", "jest.config.ts", "**/*.test.ts"],
  "include": ["**/*.ts"]
}
